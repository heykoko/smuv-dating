lassName=\"profileDetails\">\n        You Both Like:\n        {matchingArtists.map((x) => (\n          <p>{x} </p>\n        ))}\n      </div>\n    );\n  };\n\n  return (\n    <div>\n      {people.map((person) => {\n        if (person.id !== props.user.id) {\n          return (\n            <div className=\"cardContainer\">\n              <TinderCard\n                onSwipe={onSwipe}\n                className=\"swipe\"\n                key={person.id}\n                preventSwipe={[\"up\", \"down\"]}\n              >\n                <div\n                  style={{ backgroundImage: `url(${person.url})` }}\n                  className=\"card\"\n                >\n                  <h3>\n                    {person.name}, {person.age}\n                  </h3>\n                </div>\n                <div className=\"profileDetails\">\n                  favorite artists:\n                  <ul>\n                    {person.favoriteArtists.map((x) => (\n                      <li>{x}</li>\n                    ))}\n                  </ul>\n                </div>\n                <div>{artistMatch(person.favoriteArtists)}</div>\n              </TinderCard>\n            </div>\n          );\n        }\n      })}\n    </div>\n  );\n}\n\nexport default Cards;\n","/Users/karynho/smuv-dating/src/Header.js",[],"/Users/karynho/smuv-dating/src/Chats.js",[],"/Users/karynho/smuv-dating/src/ChatScreen.js",["53"],"import React from 'react';\nimport { useState } from \"react\";\nimport \"./ChatScreen.css\";\nimport Avatar from \"@material-ui/core/Avatar\";\n\nfunction ChatScreen() {\n    const [messages, setMessages]= useState([\n        {\n            name: \"ELON\",\n            image: \"...\",\n            message: \"Hi\",\n        }\n    ])\n    return (\n        <div className= \"chatScreen\">\n            <p>YOU MATCHED WITH ELON ON 12/12/12</p>\n            {messages.map(message => (\n                <div className=\"chatScreenMessage\"> \n                <Avatar\n           